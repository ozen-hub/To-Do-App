<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="context" prefHeight="401.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.devstack.app.controllers.LoginFormController">
   <children>
      <Label layoutX="170.0" layoutY="36.0" text="Login">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="14.0" layoutY="121.0" prefHeight="36.0" prefWidth="375.0" promptText="Username">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="14.0" layoutY="173.0" prefHeight="36.0" prefWidth="375.0" promptText="Password">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnSignin" layoutX="14.0" layoutY="233.0" mnemonicParsing="false" onAction="#btnloginOnAction" prefHeight="47.0" prefWidth="375.0" style="-fx-background-color: #3498db;" text="Signin" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Label layoutX="195.0" layoutY="304.0" text="OR" />
      <Button layoutX="16.0" layoutY="332.0" mnemonicParsing="false" onAction="#btnCreatenewAccountOnAction" prefHeight="47.0" prefWidth="375.0" style="-fx-background-color: #16a085;" text="Don't have an account" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
